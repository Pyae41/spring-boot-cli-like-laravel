  "scripts": {
    "build": "babel src --out-dir dist",
    "pkg": "pkg dist/sb-cli.js --targets node18-linux-x64,node18-macos-x64,node18-win-x64 --out-path ./sb-cli/bin --assets \"node_modules/**/*\"",
    "test": "echo \"Error: no test specified\" && exit 1"
